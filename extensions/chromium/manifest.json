{
  "manifest_version": 2,
  "name": "PDF Viewer",
  "version": "PDFJSSCRIPT_VERSION",
  "description": "Uses HTML5 to display PDF files directly in the browser.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "permissions": [
    "webRequest", "webRequestBlocking",
    "<all_urls>",
    "tabs",
    "webNavigation"
  ],
  "content_scripts": [{
    "matches": ["file://*/*"],
    "js": ["nothing.js"]
  }],
  "background": {
    "page": "pdfHandler.html"
  },
  "page_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Show PDF URL",
    "default_popup": "pageActionPopup.html"
  },
  "incognito": "split",
  "web_accessible_resources": [
    "getFrameId",
    "content/web/viewer.html",
    "http:/*",
    "https:/*",
    "ftp:/*",
    "file:/*",
    "chrome-extension:/*"
  ]
}
